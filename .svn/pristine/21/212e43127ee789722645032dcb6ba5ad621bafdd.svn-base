package com.ange.model;

   /**
    * storageTank 实体类
    *  dzjin
    */ 


public class StorageTank{
	private int id;
	private String tankColumn;
	private String storageTankNumber;
	private String storageTankPosition;
	private String storageTankCapacity;
	public void setId(int id){
	this.id=id;
	}
	public int getId(){
		return id;
	}
	public void setTankColumn(String tankColumn){
	this.tankColumn=tankColumn;
	}
	public String getTankColumn(){
		return tankColumn;
	}
	public void setStorageTankNumber(String storageTankNumber){
	this.storageTankNumber=storageTankNumber;
	}
	public String getStorageTankNumber(){
		return storageTankNumber;
	}
	public void setStorageTankPosition(String storageTankPosition){
	this.storageTankPosition=storageTankPosition;
	}
	public String getStorageTankPosition(){
		return storageTankPosition;
	}
	public void setStorageTankCapacity(String storageTankCapacity){
	this.storageTankCapacity=storageTankCapacity;
	}
	public String getStorageTankCapacity(){
		return storageTankCapacity;
	}
}

