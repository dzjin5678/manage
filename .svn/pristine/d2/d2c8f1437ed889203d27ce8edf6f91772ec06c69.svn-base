package com.ange.tool;

import java.util.UUID;

public class UuidUtil {
	
	public static String createUUID() {
		String str=UUID.randomUUID().toString().replaceAll("-", "");
		return str;
	}
	public static void main(String[] args) {
		String str=UUID.randomUUID().toString().replaceAll("-", "");
		System.out.println(str);
	}
}
