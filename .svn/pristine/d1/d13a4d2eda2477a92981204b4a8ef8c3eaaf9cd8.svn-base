package com.ange.tool;



public @interface ApiDoc {

    /**
     * result class
     * @return
     */
    Class<?> value() default Null.class;

    /**
     * result class
     */
    Class<?> result() default Null.class;

    /**
     * request url
     */
    String url() default "";

    /**
     * request method
     */
    String method() default "get";

    final class Null{

    }
}